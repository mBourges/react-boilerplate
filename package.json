{
  "name": "issue-management",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "prebuild": "rm -rf dist && mkdir dist",
    "build": "npm-run-all --parallel dist dist:min",
    "commit": "git-cz",
    "predev": "rm -rf dist && mkdir dist",
    "dev": "webpack-dev-server --config dev.config.js",
    "dist": "webpack --config dist.config.js --progress --colors",
    "dist:min": "webpack --config webpackDist.config.js --progress --colors --optimize-minimize --optimize-occurence-order --optimize-dedupe",
    "test": "ava 'app/**/*.test.js' --verbose --require ./other/setup-ava-tests.js",
    "cover": "nyc --require babel-core/register --require babel-polyfill --reporter=lcov --reporter=text --reporter=html npm run test",
    "check-coverage": "nyc check-coverage --statements 100 --branches 100 --functions 100 --lines 100"
  },
  "author": "Matt B",
  "license": "ISC",
  "devDependencies": {
    "ava": "0.12.0",
    "babel-core": "6.5.2",
    "babel-loader": "6.2.3",
    "babel-polyfill": "6.5.0",
    "babel-preset-es2015": "6.5.0",
    "babel-preset-react": "6.5.0",
    "babel-register": "6.5.2",
    "commitizen": "2.5.0",
    "css-loader": "0.23.1",
    "cz-conventional-changelog": "1.1.5",
    "expect": "1.14.0",
    "extract-text-webpack-plugin": "1.0.1",
    "html-webpack-plugin": "2.9.0",
    "npm-run-all": "1.5.1",
    "nyc": "5.6.0",
    "react-addons-test-utils": "0.14.7",
    "react-hot-loader": "1.3.0",
    "sinon": "^1.17.3",
    "style-loader": "0.13.0",
    "webpack": "1.12.13",
    "webpack-dev-server": "1.14.1"
  },
  "dependencies": {
    "axios": "0.9.1",
    "classnames": "2.2.3",
    "history": "2.0.0",
    "immutable": "3.7.6",
    "moment": "2.11.2",
    "react": "0.14.7",
    "react-dom": "0.14.7",
    "react-redux": "4.4.0",
    "react-router": "2.0.0",
    "react-router-redux": "3.0.0",
    "redux": "3.3.1",
    "redux-thunk": "1.0.3"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "nyc": {
    "exclude": [
      "other"
    ]
  }
}
