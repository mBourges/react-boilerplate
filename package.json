{
  "name": "react-boilerplate",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "check-coverage": "istanbul check-coverage --statements 0 --branches 0 --functions 0 --lines 0",
    "copy-files": "cp ./app/favicon.ico dist",
    "clean": "rm -rf dist && mkdir dist",
    "clean-dist": "npm-run-all --serial clean copy-files",
    "lint": "eslint ./app",
    "prebuild": "npm run clean-dist",
    "build": "webpack --env.dev",
    "build:prod": "webpack --env.prod",
    "prestart": "npm run clean-dist",
    "start": "webpack-dev-server --env.dev --content-base dist --host $IP --port 8081",
    "start-back": "nodemon --watch server server/index.js",
    "test": "karma start",
    "test:watch": "npm run test -- --auto-watch --no-single-run",
    "validate": "npm-run-all --parallel validate-webpack:* lint test --serial check-coverage",
    "validate-webpack:dev": "webpack-validator webpack.config.js --env.dev",
    "validate-webpack:prod": "webpack-validator webpack.config.js --env.prod"
  },
  "author": "Matt B",
  "license": "ISC",
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-core": "^6.9.1",
    "babel-eslint": "^6.0.4",
    "babel-loader": "^6.2.4",
    "babel-plugin-__coverage__": "^11.0.0",
    "babel-preset-es2015-webpack": "^6.4.1",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-2": "^6.5.0",
    "chai": "^3.5.0",
    "css-loader": "^0.23.1",
    "es6-promise": "^3.2.1",
    "eslint": "^2.13.0",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-import-resolver-webpack": "^0.3.0",
    "eslint-loader": "^1.3.0",
    "eslint-plugin-import": "^1.8.1",
    "eslint-plugin-jsx-a11y": "^1.5.3",
    "eslint-plugin-react": "^5.2.2",
    "html-webpack-plugin": "^2.21.0",
    "istanbul": "^0.4.4",
    "karma": "^0.13.22",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^1.0.1",
    "karma-coverage": "^1.0.0",
    "karma-mocha": "^1.0.1",
    "karma-phantomjs-launcher": "^1.0.0",
    "karma-webpack": "^1.7.0",
    "mocha": "^2.5.3",
    "nodemon": "^1.9.2",
    "npm-run-all": "^2.2.0",
    "path": "^0.12.7",
    "phantomjs-prebuilt": "^2.1.7",
    "style-loader": "^0.13.1",
    "webpack": "^2.1.0-beta.6",
    "webpack-dev-server": "^1.14.1",
    "webpack-validator": "^2.1.3"
  },
  "dependencies": {
    "babel-polyfill": "^6.9.1",
    "chalk": "^1.1.3",
    "classnames": "^2.2.5",
    "good": "^7.0.1",
    "good-console": "^6.1.2",
    "good-squeeze": "^4.0.0",
    "hapi": "^13.4.1",
    "history": "^3.0.0",
    "immutable": "^3.8.1",
    "inert": "^4.0.0",
    "ip": "^1.1.3",
    "local-storage": "^1.4.2",
    "moment": "^2.13.0",
    "react": "^15.1.0",
    "react-dom": "^15.1.0",
    "react-redux": "^4.4.5",
    "react-router": "^2.4.1",
    "react-router-redux": "^4.0.5",
    "redux": "^3.5.2",
    "redux-immutable": "^3.0.6",
    "redux-saga": "^0.10.5",
    "reselect": "^2.5.1",
    "whatwg-fetch": "^1.0.0"
  },
  "config": {}
}
